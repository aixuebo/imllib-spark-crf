### 模板文件
# Unigram
U01:%x[-4,0]
U02:%x[-3,0]
U03:%x[-2,0]
U04:%x[-1,0]
U05:%x[0,0]
U06:%x[1,0]
U07:%x[2,0]
U08:%x[3,0]
U9:%x[4,0]
U10:%x[-1,0]/%x[0,0]
U11:%x[-2,0]/%x[0,0]/%x[0,0]
U12:%x[-3,0]/%x[-2,0]/%x[-1,0]
U13:%x[-4,0]/%x[-3,0]/%x[-2,0]
U14:%x[0,0]/%x[1,0]
U15:%x[0,0]/%x[1,0]/%x[2,0]
U16:%x[1,0]/%x[2,0]/%x[3,0]
U17:%x[2,0]/%x[3,0]/%x[4,0]
